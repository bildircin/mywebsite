                        <div class="search-item city pull-left">
                            <p><%= currentLang.toursPageSearchContentCategory %></p>
                            <select name="category">
                                <option value="null"><%= currentLang.toursPageSearchContentCategory %></option>
                                <% categories.forEach(item => { %>
                                    <option value="<%= item.id %>"><%= item.name %></option>
                                <% }) %> 
                            </select>
                        </div>
                        <div class="search-item date pull-left">
                            <p><%= currentLang.toursPageSearchContentDateStart %></p>
