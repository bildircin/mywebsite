                            <button type="submit" class="button"><%= currentLang.toursPageSearchContentButton %> <i class="fa fa-angle-right" aria-hidden="true"></i></button>
                            <input type="hidden" name="search_tour" value="1">
                        </div>
                    </form>
                </div>
            </div>
