                        <div class="search-item date pull-left">
                            <p><%= currentLang.toursPageSearchContentDateEnd %></p>
                            <input type="text" class="tour-search-input datepicker hasDatepicker" name="finishedAt" id="date_to" placeholder="DD/MM/YY">
                        </div>
                        <div class="search-item item-select pull-left">
                            <p><%= currentLang.toursPageSearchContentPersons %></p>
                            <select name="persons">
                                <option value="0"><%= currentLang.toursPageSearchContentPersons %></option>
                                <option value="1">1 people</option>
                                <option value="2">2 people</option>
                                <option value="3">3 people</option>
                                <option value="4">4 people</option>
                            </select>
                        </div>
                        <div class="search-item item-budget pull-left">
                            <p><%= currentLang.toursPageSearchContentMaxPrice %></p>
                            <input type="number" class="hotel-budget-input" name="price" id="budget" placeholder="<%= currentLang.toursPageSearchContentMaxPrice %>" >
